{
  "name": "gbskiro5",
  "version": "1.0.0",
  "description": "This repository hosts static web pages and assets used for GBS AI learning resources, including workshop materials and a prompt library.",
  "main": ".eleventy.js",
  "scripts": {
    "dev": "eleventy --serve",
    "build": "eleventy && vite build",
    "build:css": "npx tailwindcss -i ./shared/styles/base.css -o ./shared/styles/tailwind.css --minify",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky install",
    "test": "vitest",
    "test:e2e": "playwright test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "vite": "^5.3.4",
    "@playwright/test": "^1.46.0",
    "jsdom": "^24.0.0",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
